<% provide(:title, "Group") %>
<h1>Group</h1>
<p>Find me in app/views/admin_static_pages/groupPage.html.erb</p>

<table>
  <thead>
    <tr>
      <th>Your Groups</th>
    </tr>
  </thead>

    <tbody>
        <% @admin_groups.each do |group| %>
        <tr>
            <td><%= link_to group.g_name,  admin_static_pages_groupMemberPage_path  %></td>
        </tr>
        <% end %>
    </tbody>
</table>
