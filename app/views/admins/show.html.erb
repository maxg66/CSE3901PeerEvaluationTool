<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @admin.a_name %>
</p>

<p>
<strong>Email:</strong>
  <%= @admin.a_email %>
</p>

<button type="button"><%= link_to 'Create Another Admin', new_admin_path %></button>
<button type="button"><%= link_to 'Projects', projects_path %></button>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Current Admins</th>
      <th>Email</th>
    </tr>
  </thead>

    <tbody>
        <% @admins.each do |admin| %>
        <tr>
            <td><%= admin.a_name %></td>
            <td><%= admin.a_email %></td>
        </tr>
        <% end %>
    </tbody>
</table>

<%= render 'layouts/footer' %>