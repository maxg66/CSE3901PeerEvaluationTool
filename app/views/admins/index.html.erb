<header>
<%= render 'layouts/header' %>
</header>



<div class="centerBackground">


<h1>Current Admins</h1>

<%= link_to_primary_button 'Back to Projects', projects_path %>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>

    <tbody>
        <% @admins.each do |admin| %>
        <tr>
            <td><%= admin.a_name %></td>
            <td><%= admin.a_email %></td>
            <td><%= link_to_button 'Edit', edit_admin_path(admin) %></td>
            <td><a href=<%= admin_path(admin)%> class="btn btn-danger btn-lg btn-block" data-confim="Are you sure?" data-method="delete" rel="nofollow">Delete</a></td>
        </tr>
        <% end %>
    </tbody>
</table>
</div>
<%= render 'layouts/footer' %>