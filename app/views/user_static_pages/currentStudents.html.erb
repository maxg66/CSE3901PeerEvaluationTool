<header>
<%= render 'layouts/header' %>
</header>
<div class="centerBackground">

<h1>Current Students</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>

    <tbody>
        <% @users.each do |user| %>
        <tr>
            <td><%= user.u_name %></td>
            <td><%= user.email %></td>
        </tr>
        <% end %>
    </tbody>
</table>
</div>
<%= render 'layouts/footer' %>